{
  "name": "@tensamin/authserver",
  "version": "0.1.0",
  "files": [
    "src/schema.ts",
    "drizzle"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "type": "module",
  "scripts": {
    "start": "bun --bun run src/main.ts",
    "dev:raw": "bun --bun run --watch src/main.ts",
    "dev": "docker compose down && docker compose up -d",
    "lint": "eslint",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio --host 0.0.0.0 --port 15692"
  },
  "dependencies": {
    "@sentry/bun": "^10.29.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.45.0",
    "livekit-server-sdk": "^2.14.1",
    "mysql2": "^3.9.4",
    "sharp": "^0.33.4",
    "uuid": "^13.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.10.0",
    "bun-types": "latest",
    "drizzle-kit": "^0.31.8",
    "drizzle-zod": "^0.8.3",
    "eslint": "^9.11.1",
    "typescript": "^5.4.0"
  }
}
